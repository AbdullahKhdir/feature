<!DOCTYPE html>
<html lang="en">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="keywords" content="HTML, CSS, JavaScript">                  <!-- Search Keywords --> 
    <meta name="description" content="Full Node JS Framework">              <!-- Search Description -->
    <meta name="author" content="Abdullah Khdir">                           <!-- Search Author -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">  <!-- Width Adjustments for all screens -->
    <!-- <meta http-equiv="refresh" content="60">                           refreshing the page every 60 seconds-->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="msapplication-tap-highlight" content="no">
    <meta name="description" content="">
    <title><%= nav_title %></title>
    <link href="../materialize/datatables.min.css" rel="stylesheet">
    <link href="../materialize/jqvmap.css" rel="stylesheet">
    <link href="../materialize/flag-icon-css/css/flag-icon.min.css" rel="stylesheet">
    <!-- Fullcalendar-->
    <link href="../materialize/fullcalendar.min.css" rel="stylesheet">
    <!-- Materialize-->
    <link href="../materialize/admin-materialize.min.css" rel="stylesheet">
    <!-- Material Icons-->
    <link href="../materialize/material_icons.css" rel="stylesheet">
    <!-- ####################################################################### -->
    <link rel="stylesheet" href="../css/cube.css">
    <link rel="stylesheet" href="../plugins/materialize/css/materialize.css"> 
    <!-- ####################################################################### -->
    <% if (typeof js !== 'undefined') { %>
      <% if (js.length > 0) { %>
          <% js.forEach(file => { %>
              <script type="text/javascript" src="/<%= file.toString() %>" defer></script>
          <% }) %>
      <% } %>
  <% } %>
  <% if (typeof css !== 'undefined') { %>
      <% if (css.length > 0) { %>
          <% css.forEach(file => { %>
            <link rel="stylesheet" href="../<%= file.toString() %>"> 
          <% }) %>
      <% } %>
  <% } %>